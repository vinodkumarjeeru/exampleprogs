<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.model.Address" table="address">
		<id column="addressid" name="addressId" type="java.lang.Integer">
			<generator class="native" />
		</id>

		<property column="city" name="city" type="java.lang.String" />
		<property column="country" name="country" type="java.lang.String" />




	</class>



</hibernate-mapping>